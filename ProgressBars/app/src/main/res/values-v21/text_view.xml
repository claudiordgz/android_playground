<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="mapps_text_list_view" >
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="mapps_text_view" >
        <item name="android:typeface">monospace</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">25sp</item>
    </style>
    <style name="mapps_text_view.Main">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/gradient_title</item>
    </style>
    <style name="mapps_text_view.Subtitle">
        <item name="android:background">@drawable/gradient_subtitle</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>